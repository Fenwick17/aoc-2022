{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "lib": ["dom", "dom.iterable"]
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
